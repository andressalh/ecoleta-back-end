{
  "type": "module",
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --transpileOnly --ignore-watch node_modules src/server.ts",
    "knex:seed": "npx knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "npx knex --knexfile knexfile.ts migrate:latest",
    "heroku-prebuild": "echo This runs before Heroku installs dependencies.",
    "heroku-cleanup": "echo This runs after Heroku prunes and caches dependencies.",
    "build": "tsc -p .",
    "start": "node ./dist/src/server.js",
    "heroku-postbuild": "npx knex --knexfile ./dist/src/knexfile.ts migrate:latest && npx knex --knexfile ./dist/src/knexfile.ts seed:run "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.3",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "multer": "^1.4.2",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
